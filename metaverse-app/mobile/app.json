{
  "expo": {
    "name": "MetaLab",
    "slug": "metaverselaboratory",
    "scheme": "metaverselaboratory",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1020"
    },

    "ios": {
      "bundleIdentifier": "com.metaverse.lab",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.metaverse.lab",
      "adaptiveIcon": {
        "foregroundImage": "./assets/roga.png",
        "backgroundColor": "#0B1020"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/roga.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "@react-native-community/datetimepicker",
      "expo-web-browser"
    ],

    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1633a55-84a6-48be-aed6-bb714c35eb7e"
      }
    }
  }
}
